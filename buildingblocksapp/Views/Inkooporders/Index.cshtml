@model IEnumerable<buildingblocksapp.Models.Inkooporder>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PeriodeBinnenkomst)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PeriodeVerwerkt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rood)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Grijs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blauw)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InkooporderCorrectie)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PeriodeBinnenkomst)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PeriodeVerwerkt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rood)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Grijs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blauw)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InkooporderCorrectie.InkooporderCorrectieId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InkooporderId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InkooporderId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InkooporderId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
